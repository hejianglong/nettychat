<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.im.nettychat.domain.mapper.ResourceMapper">
    <select id="findValuesByType" resultType="Resource">
        select `value` from `resource` where `type` = #{type}
    </select>
</mapper>